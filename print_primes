import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		int i,j;
		for(i=2;i<=n;i++)
		{ int a=0;
			for(j=1;j<=i;j++)
			{
				if(i%j==0)
			
				{
			    a++;
				}
				
				 
				 
			}
			if(a==2)
				
			{
		    System.out.println(i);
			}
		}
    }
}
      
